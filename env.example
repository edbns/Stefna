# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# AI Provider Configuration
AIML_PROVIDER=your_ai_provider_name
AIML_API_KEY=your_ai_api_key

# Netlify Configuration
NETLIFY_DEV=true
FUNCTION_APP_KEY=your_function_app_key_here

# JWT Secret (unified across all functions)
JWT_SECRET=your-long-random-jwt-secret-here

# Note: AUTH_JWT_SECRET is deprecated. Set it equal to JWT_SECRET until migration is complete.

# Client-side Environment Variables (VITE_*)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
VITE_FUNCTION_APP_KEY=your_function_app_key_here 