# Environment Variables for Stefna
# Copy this file to .env and fill in your values

# AIML API Configuration (Frontend - Vite)
VITE_AIMLAPI_API_KEY=your_aiml_api_key_here

# AIML API Configuration (Netlify Functions - Backend)
# These are the variables that Netlify Functions can access
AIML_API_KEY=your_aiml_api_key_here
AIML_API_URL=https://api.aimlapi.com

# Backend Configuration (Netlify environment variables)
# RESEND_API_KEY=your_resend_api_key_here
# SUPABASE_URL=your_supabase_url_here
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
# JWT_SECRET=your_jwt_secret_key_here

# App Configuration
VITE_APP_ENV=development
VITE_DEBUG_MODE=false

# Note: 
# - Frontend uses VITE_* variables (accessible via import.meta.env)
# - Netlify Functions use regular variables (accessible via process.env)
# - Set AIML_API_KEY and AIML_API_URL in Netlify environment variables
# - Remove any VITE_* variables from Netlify Functions environment 